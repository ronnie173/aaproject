<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/booking_details_full_view_constraint_layout"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/booking_details_full_view_activity_booking_details_full_view_constraint_layout_background_color">

	<androidx.recyclerview.widget.RecyclerView
		android:id="@+id/recycler_recycler_view"
		android:layout_width="0dp"
		android:layout_height="@dimen/booking_details_full_view_activity_recycler_recycler_view_height"
		android:layout_marginTop="@dimen/booking_details_full_view_activity_recycler_recycler_view_margin_top"
		app:layout_constraintHorizontal_weight="1"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="92dp"
		tools:listitem="@layout/search_results_view_list_item"/>
</androidx.constraintlayout.widget.ConstraintLayout>